{
    "name": "bundana/laravel-sms-notify",
    "description": "A flexible and powerful SMS notification package for Laravel applications",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bundana\\LaravelSmsNotify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bundana\\LaravelSmsNotify\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-phpunit": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bundana\\LaravelSmsNotify\\LaravelSmsNotifyServiceProvider"
            ]
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "version": "1.0.0-beta.3"
}
